{
  "manifest_version": 3,
  "name": "MarkMind",
  "version": "2.0.0",
  "description": "AI-powered bookmark organization that helps you keep your bookmarks clean and organized using artificial intelligence.",
  "permissions": [
    "bookmarks",
    "activeTab",
    "tabs",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*",
    "https://api.openai.com/*",
    "https://api.anthropic.com/*",
    "https://openrouter.ai/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  }
} 